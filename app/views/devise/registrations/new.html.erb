  <header class="aci59a-0 eyAIuG haute sticky-limit">
    <div class="sc-1gnwgfb-2 stom">
    </div>
       <div class="head-te"><marquee behavior="" direction="">La communauté internationale réaffirme son soutient aux pays les plus pauvres</marquee></div>
  </header>

<div class="mx-auto sign-up p-4">
  <h2 class="text-center">Inscrivez-vous</h2>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name), class:'form') do |f| %>
    
      <ol>
        <li><%= render "devise/shared/error_messages", resource: resource %></li>
      </ol>
  <div class="field">
    <%= f.label :first_name, "Nom" %><br />
    <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name", class:'form-control'%>
  </div>

  <div class="field">
    <%= f.label :last_name, "Prénoms" %><br />
    <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name", class:'form-control' %>
  </div>

  <div class="field">
    <%= f.label :phone, "Téléphone" %><br />
    <%= f.text_field :phone, autofocus: true, autocomplete: "phone", class:'form-control'%>
  </div>

  <div class="field">
     <%= f.label :Organisme, "Organisme"%><br />
     <%= f.select :organisme_id, Organisme.all.collect { |o| [ o.nom, o.id ] }, include_blank: true%>
  </div>


  <div class="field">
    <%= f.label :email, "Email" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class:'form-control' %>
  </div>

  <div class="field">
    <%= f.label :password, "Mot de passe" %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password", class:'form-control' %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation, "Entrez le mot de passe à nouveau" %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password", class:'form-control' %>
  </div>



  <div>
    <p>
    <%= f.radio_button :is_admin, false, :class => ''%>
    <%= f.label :is_admin, "Je suis bailleur", :value => "false"  %>
    <%= f.radio_button :is_admin, nil, :checked => true, :class => ''%>
    <%= f.label :is_admin, "Je suis visiteur", :value => "nil" %>

    </p>
 
  </div>

  <div class="actions">
    <%= f.submit "S'inscrire", class:'sc-eerKOB cfnPHQ mt-3' %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
  
</div>
