
<div class="container sans-head mb-2">
	<div class="text-center text-muted mt-4 mb-4">
		<h3 class="tablePrint"> Liste des projets modifiÃ©s par l'administrateur</h3>
	</div>
	<div class="container mb-5">
		<div class="row">
			<% @projets.each do |p|%>
			<div class="col-md-4 mb-3">
				<div class="card card-height position-relative">
					<div class="card-body">
						<h5 class="card-title"><%= p.nom_du_projet.truncate(40) %></h5>
						<p class="card-text"><%= raw truncate(sanitize(p.objectif_generale_du_projet, tags: []), length: 100)%></p>
						<div class="row align-items-center mt-2 position-absolute card-foot">
							<a href="<%= super_admin_update_projet_path(p.id)%>" class="col tablePrint mb-0">Voir</a>
						</div>	
					</div>
				</div>
			</div>
			<%end%>	
		</div>
		<div class="text-center">
			<%= paginate @projets %>
		</div>
	</div>
</div>