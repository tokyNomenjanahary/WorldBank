<div class="row">

	<% @projets.each do |p|%>
		  <div class="col-sm-6">
		    <div class="card">
		    	
		      <div class="card-body">
		        <h5 class="card-title"><%= p.nom_du_projet %></h5>
		        <p class="card-text"><%= p.objectif_generale_du_projet %></p>
		        <%if p.picture.attached?%>
				    	<%= image_tag p.picture, alt: 'picture', class:"card-img-circle"%>
				<%else%>
					<img class="card-img-top" width="100px" height="300px" src="" alt="Card image cap">
				<%end%>
		    	
		        <a href="<%= admin_projet_path(p.id)%>" class="btn btn-primary">Voir</a>
		        <a href="<%= edit_admin_projet_path(p.id)%>" class="btn btn-warning">Modifier</a>
		        <a href="<%= admin_projet_path(p.id)%>" class="btn btn-danger" data-method="delete" data-confirm="Voulez-vous refuser ce projet?">Refuser</a>
		      </div>
		    </div>
		  </div>

	<%end%>

</div>